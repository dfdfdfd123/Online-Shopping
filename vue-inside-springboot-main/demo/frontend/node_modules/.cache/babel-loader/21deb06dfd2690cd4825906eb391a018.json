{"ast":null,"code":"export default {\n  data() {\n    return {\n      username: '',\n      password: '',\n      email: '',\n      name: '',\n      phone: '',\n      gender: '',\n      agreements: [],\n      allAgreements: false\n    };\n  },\n\n  methods: {\n    toggleAllAgreements() {\n      this.agreements.forEach(agreement => {\n        agreement.checked = this.allAgreements;\n      });\n    },\n\n    registerUser() {\n      if (!this.agree) {\n        alert('약관에 동의해주세요.');\n        return;\n      } // 회원가입 로직 수행\n\n\n      console.log('아이디:', this.username);\n      console.log('이메일:', this.email);\n      console.log('비밀번호:', this.password); // 폼 필드 초기화\n\n      this.username = '';\n      this.password = '';\n      this.email = '';\n      this.name = '';\n      this.phone = '';\n      this.agree = false;\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAuDA,eAAe;AACbA,MAAI,GAAG;AACL,WAAO;AACLC,cAAQ,EAAE,EADL;AAELC,cAAQ,EAAE,EAFL;AAGLC,WAAK,EAAE,EAHF;AAILC,UAAI,EAAE,EAJD;AAKLC,WAAK,EAAE,EALF;AAMLC,YAAM,EAAE,EANH;AAOLC,gBAAU,EAAE,EAPP;AAULC,mBAAa,EAAE;AAVV,KAAP;AAYD,GAdY;;AAebC,SAAO,EAAE;AACPC,uBAAmB,GAAG;AACpB,WAAKH,UAAL,CAAgBI,OAAhB,CAAyBC,SAAD,IAAe;AACrCA,iBAAS,CAACC,OAAV,GAAoB,KAAKL,aAAzB;AACD,OAFD;AAGD,KALM;;AAMPM,gBAAY,GAAG;AACb,UAAI,CAAC,KAAKC,KAAV,EAAiB;AACfC,aAAK,CAAC,aAAD,CAAL;AACA;AACF,OAJa,CAKb;;;AACAC,aAAO,CAACC,GAAR,CAAY,MAAZ,EAAoB,KAAKjB,QAAzB;AACAgB,aAAO,CAACC,GAAR,CAAY,MAAZ,EAAoB,KAAKf,KAAzB;AACAc,aAAO,CAACC,GAAR,CAAY,OAAZ,EAAqB,KAAKhB,QAA1B,EARa,CAUb;;AACA,WAAKD,QAAL,GAAgB,EAAhB;AACA,WAAKC,QAAL,GAAgB,EAAhB;AACA,WAAKC,KAAL,GAAa,EAAb;AACA,WAAKC,IAAL,GAAY,EAAZ;AACA,WAAKC,KAAL,GAAa,EAAb;AACA,WAAKU,KAAL,GAAa,KAAb;AACF;;AAvBO;AAfI,CAAf","names":["data","username","password","email","name","phone","gender","agreements","allAgreements","methods","toggleAllAgreements","forEach","agreement","checked","registerUser","agree","alert","console","log"],"sourceRoot":"","sources":["C:\\Users\\winwi\\Downloads\\vue3\\vue-inside-springboot-main\\demo\\frontend\\src\\views\\SignUp.vue"],"sourcesContent":["<template>\r\n  <v-container fluid class=\"registration-form\">\r\n    <v-row justify=\"center\">\r\n      <v-col cols=\"12\" sm=\"8\" md=\"6\">\r\n        <h2>회원가입</h2>\r\n        <v-form @submit.prevent=\"registerUser\">\r\n          <v-row>\r\n            <v-col cols=\"12\" sm=\"6\">\r\n              <v-text-field v-model=\"username\" label=\"아이디\" required></v-text-field>\r\n            </v-col>\r\n            <v-col cols=\"12\" sm=\"6\">\r\n              <v-text-field v-model=\"password\" label=\"비밀번호\" type=\"password\" required></v-text-field>\r\n            </v-col>\r\n          </v-row>\r\n          <v-row>\r\n            <v-col cols=\"12\" sm=\"6\">\r\n              <v-text-field v-model=\"name\" label=\"이름\" required></v-text-field>\r\n            </v-col>\r\n            <v-col cols=\"12\" sm=\"6\">\r\n              <v-radio-group v-model=\"gender\" row>\r\n                <v-radio label=\"남성\" value=\"male\"></v-radio>\r\n                <v-radio label=\"여성\" value=\"female\"></v-radio>\r\n              </v-radio-group>\r\n            </v-col>\r\n          </v-row>\r\n          <v-row>\r\n            <v-col cols=\"12\" sm=\"6\">\r\n              <v-text-field v-model=\"phone\" label=\"휴대폰\" required></v-text-field>\r\n            </v-col>\r\n            <v-col cols=\"12\" sm=\"6\">\r\n              <v-text-field v-model=\"email\" label=\"이메일\" required></v-text-field>\r\n            </v-col>\r\n          </v-row>\r\n          <v-row>\r\n            <v-col cols=\"12\">\r\n              <v-checkbox v-for=\"agreement in agreements\" :key=\"agreement.id\" v-model=\"agreement.checked\" :label=\"agreement.label\" :rules=\"[v => !!v || '동의해주세요']\"></v-checkbox>\r\n            </v-col>\r\n          </v-row>\r\n          <v-row>\r\n            <v-col cols=\"12\">\r\n              <v-checkbox v-model=\"allAgreements\" @change=\"toggleAllAgreements\" label=\"모두 확인하였으며 동의합니다\"></v-checkbox>\r\n            </v-col>\r\n          </v-row>\r\n          <v-row>\r\n            <v-col cols=\"12\">\r\n              <v-btn type=\"submit\" :disabled=\"!agree\" color=\"primary\">Register</v-btn>\r\n            </v-col>\r\n          </v-row>\r\n        </v-form>\r\n      </v-col>\r\n    </v-row>\r\n  </v-container>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      username: '',\r\n      password: '',\r\n      email: '',\r\n      name: '',\r\n      phone: '',\r\n      gender: '',\r\n      agreements: [\r\n        \r\n      ],\r\n      allAgreements: false\r\n    };\r\n  },\r\n  methods: {\r\n    toggleAllAgreements() {\r\n      this.agreements.forEach((agreement) => {\r\n        agreement.checked = this.allAgreements;\r\n      });\r\n    },\r\n    registerUser() {\r\n      if (!this.agree) {\r\n        alert('약관에 동의해주세요.');\r\n        return;\r\n      }\r\n      // 회원가입 로직 수행\r\n      console.log('아이디:', this.username);\r\n      console.log('이메일:', this.email);\r\n      console.log('비밀번호:', this.password);\r\n\r\n      // 폼 필드 초기화\r\n      this.username = '';\r\n      this.password = '';\r\n      this.email = '';\r\n      this.name = '';\r\n      this.phone = '';\r\n      this.agree = false;\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.registration-form {\r\n  max-width: 400px;\r\n  margin: 0 auto;\r\n  padding: 20px;\r\n  border-radius: 4px;\r\n}\r\n\r\n.form-group {\r\n  margin-bottom: 20px;\r\n}\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}