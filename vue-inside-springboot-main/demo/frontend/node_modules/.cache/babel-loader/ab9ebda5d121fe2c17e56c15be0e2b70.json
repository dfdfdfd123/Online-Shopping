llAgreements() {\r\n      this.agreements.forEach((agreement) => {\r\n        agreement.checked = this.allAgreements;\r\n      });\r\n    },\r\n    registerUser() {\r\n      if (!this.agree) {\r\n        alert('약관에 동의해주세요.');\r\n        return;\r\n      }\r\n      // 회원가입 로직 수행\r\n      console.log('아이디:', this.username);\r\n      console.log('이메일:', this.email);\r\n      console.log('비밀번호:', this.password);\r\n\r\n      // 폼 필드 초기화\r\n      this.username = '';\r\n      this.password = '';\r\n      this.email = '';\r\n      this.name = '';\r\n      this.phone = '';\r\n      this.agree = false;\r\n    }\r\n  },\r\n  mounted\r\n};\r\n</script>\r\n\r\n"]},"metadata":{},"sourceType":"module"}